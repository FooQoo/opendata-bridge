{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint & stylelint src/**/*.{css,scss,sass}",
    "lint:fix": "next lint --fix",
    "stylelint:fix": "stylelint src/**/*.{css,scss,sass} --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "1.2.0",
    "next": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.29.0",
    "eslint-config-next": "^13.0.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "prettier": "2.8.0",
    "sass": "1.56.1",
    "stylelint": "14.16.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-scss": "4.3.0",
    "typescript": "4.9.3"
  }
}
