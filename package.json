{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint & stylelint src/**/*.{css,scss,sass}",
    "lint:fix": "next lint --fix",
    "stylelint:fix": "stylelint src/**/*.{css,scss,sass} --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "1.4.0",
    "next": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "2.2.0"
  },
  "devDependencies": {
    "@types/node": "18.17.5",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "eslint": "8.47.0",
    "eslint-config-next": "^13.0.3",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "sass": "1.65.1",
    "stylelint": "15.10.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-scss": "5.1.0",
    "typescript": "5.1.6"
  }
}
