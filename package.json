{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint & stylelint src/**/*.{css,scss,sass}",
    "lint:fix": "next lint --fix",
    "stylelint:fix": "stylelint src/**/*.{css,scss,sass} --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "1.3.4",
    "next": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "2.1.0"
  },
  "devDependencies": {
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-next": "^13.0.3",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "sass": "1.58.3",
    "stylelint": "15.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-scss": "4.4.0",
    "typescript": "4.9.5"
  }
}
